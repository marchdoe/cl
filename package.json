    "standard": "^10.0.3",
    "standard-loader": "^6.0.1"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "exclude": [
      "test",
      "docs",
      "stories"
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
